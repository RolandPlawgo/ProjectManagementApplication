@model ErrorViewModel
@{
    Layout = "_Layout";
}
<div class="container my-6 text-center">
    <h1>Page not found (404)</h1>
    <p>
        <a class="btn btn-primary" href="/">Go to homepage</a>
        <a class="btn btn-outline-secondary" href="/search">Search site</a>
    </p>
    <hr />
    <p class="text-muted">Reference code: <code>@Model.RequestId</code></p>
</div>
